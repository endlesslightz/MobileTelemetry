<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sartika.telemetri3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mobile Telemetri</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="nurcahya.pradana@gmail.com" href="https://about.me/nurcahyapradana">
       Nurcahya @ Sartika Mitrasejati
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <access origin="*" />
    <access origin="http://maps.google.com" />
</widget>
